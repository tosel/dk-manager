<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.villigst.dk.controller.NewMemberDialogController">
   <children>
      <Label layoutX="33.0" layoutY="28.0" text="Teilnehmer hinzufügen">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="member_dialog_name" layoutX="22.0" layoutY="105.0" prefHeight="25.0" prefWidth="256.0" promptText="Name">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="member_dialog_konvent" layoutX="22.0" layoutY="155.0" prefHeight="25.0" prefWidth="256.0" promptText="Konvent">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="member_dialog_amt" disable="true" layoutX="22.0" layoutY="241.0" prefHeight="37.0" prefWidth="256.0" promptText="Amt">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <CheckBox fx:id="member_dialog_cb_gremium" layoutX="22.0" layoutY="210.0" mnemonicParsing="false" onAction="#member_check_gremium" text="Gremium">
         <font>
            <Font size="16.0" />
         </font>
      </CheckBox>
      <Button layoutX="22.0" layoutY="327.0" mnemonicParsing="false" onAction="#member_dialog_add" prefHeight="25.0" prefWidth="256.0" text="Hinzufügen">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
